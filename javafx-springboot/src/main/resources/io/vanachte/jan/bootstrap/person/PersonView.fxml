<AnchorPane prefHeight="300.0" prefWidth="900.0" stylesheets="@../styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.vanachte.jan.bootstrap.person.PersonJavaFxController">
